function() {
  // Substitua 'productIdVariable' pelo nome da variável do Data Layer que contém o ID do produto
  var productId = {{productIdVariable}};
  
  // Verifica se o ID do produto está disponível
  if (!productId) {
    return null;
  }

  return {
    id: productId,
    google_business_vertical: 'retail'
  };
}
